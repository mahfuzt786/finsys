	<queryString>
		<![CDATA[Select t.ledgerid,l.ledgername,s.soegroupname,sm.soemaingroupname,sum(t.issuequantity*t.itemvalue) as ledgertotal, R1.itemid ,R1.reqquantity,R1.issuequantity,R1.itemvalue,R1.itemcode,R1.itemname,R1.uomabbr,R1.t,R1.ledgercode,R1.ledgername
 from finsys.t_issue_items t

left outer join finsys.m_ledger l on l.ledgerid=t.ledgerid
left outer join finsys.m_soegroup s on s.soegroupid=l.soegroupid
left outer join finsys.m_soemaingroup sm on sm.soemaingroupid=l.soemaingroupid
inner join(Select t.itemid ,sum(t.reqquantity) as reqquantity,sum(t.issuequantity) as issuequantity,sum(t.itemvalue)as itemvalue,i.itemcode,i.itemname,u.uomabbr,sum(t.issuequantity*t.itemvalue) as t,l.ledgercode,l.ledgername
 from finsys.t_issue_items t
inner join finsys.t_issue_return ir on ir.issue_returncode=t.issue_returncode
INNER JOIN finsys.m_item i on i.itemid=t.itemid
inner join finsys.t_uom u on u.uomcode=i.uomcode
left outer join finsys.m_ledger l on l.ledgerid=t.ledgerid
where ir.issuedate>=$P{startdate} and ir.issuedate<=$P{enddate}
group by t.itemid ,i.itemcode,i.itemname,u.uomabbr,l.ledgercode,l.ledgername)R1 on R1.itemid=t.itemid

group by t.ledgerid, R1.itemid ,R1.reqquantity,R1.issuequantity,R1.itemvalue,R1.itemcode,R1.itemname,R1.uomabbr,R1.t,R1.ledgercode,R1.ledgername,l.ledgername,s.soegroupname,sm.soemaingroupname]]>
	</queryString>
